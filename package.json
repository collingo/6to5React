{
  "name": "6to5test",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run serveDev & npm run watch && fg",
    "serveDev": "cd dev && python -m SimpleHTTPServer",
    "serveBuild": "cd build && python -m SimpleHTTPServer 8001",
    "watch": "onchange 'src/index.js' -- npm run compile",
    "compile": "browserify -d -t 6to5ify -t reactify src/index.js > dev/index.js",
    "release": "./tasks minify"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "6to5ify": "^3.1.2",
    "browserify": "^8.1.1",
    "envify": "^3.2.0",
    "minifyify": "^6.0.0",
    "minimist": "^1.1.0",
    "onchange": "0.0.2",
    "react": "^0.12.2",
    "reactify": "^0.17.1"
  }
}
